        window.addEventListener('DOMContentLoaded', function() {
          var api_key = 'O5SLs1K1RfWhBYkHvMaXTg'; // pragma: whitelist secret
          var forum_id = '246775';

          // Include the UserVoice JavaScript SDK (only needed once on a page)
